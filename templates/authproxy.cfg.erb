# Managed by Puppet.

<% scope['duo_authproxy::settings'].map do |k,v| -%>
<%   if v.is_a?(Hash) -%>
[<%=  k %>]
<%     v.map do |ki, vi| -%>
<%       if vi.is_a?(Array) -%>
<%=        ki %>=<%= vi.join(',') %>
<%       elsif vi != :undef -%>
<%=        ki %>=<%= vi %>
<%       end -%>
<%     end -%>

<%   else -%>
[<%=  k %>]

<%   end -%>
<% end -%>
